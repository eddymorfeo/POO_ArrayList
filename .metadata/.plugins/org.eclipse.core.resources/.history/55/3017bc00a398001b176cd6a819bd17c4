package modelo;

public class Persona {

	private int id;
	private String nombre;
	private char sexo;
	
	public Persona() {
		
	}
	
	public Persona( int idparametro, String nombreparametro, char sexoparametro) {
		this.id=idparametro;
		this.nombre= nombreparametro;
		this.sexo=sexoparametro;
				
	}
	//Geter y settr siempre son de los atributos de la clase
	
	public int getId(){
		return this.id;
	}
	//void no retorna nada en los setter
	public void setId(int idparametro) {
		this.id=idparametro;
	}
	
	
	public String getNombre() {
		return this.nombre;
	}
	
	public void setnombre(String nombre) {
		this.nombre=nombre;
		
	}
	
	//nivelacceso-->datoqueretorna-->nombrecreado(){}
}
